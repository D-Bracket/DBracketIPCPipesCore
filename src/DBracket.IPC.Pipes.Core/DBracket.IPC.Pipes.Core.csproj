<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Product>IPCPipesBase</Product>
    <Company />
    <Authors>D-Bracket</Authors>
    <Description>Base library for IPC via pipes.</Description>
    <PackageTags>IPC, Pipes, DataExchange, Data, Transfer</PackageTags>
    <PackageReleaseNotes>Creation of the package</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <NeutralLanguage>en</NeutralLanguage>
    <RepositoryUrl>https://github.com/D-Bracket/DBracketIPCPipesCore</RepositoryUrl>
    <Version>1.0.1</Version>
    <PackageProjectUrl>https://github.com/D-Bracket/DBracketIPCPipesCore</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="System.IO.Pipes" Version="4.3.0" />
    <PackageReference Include="System.IO.Pipes.AccessControl" Version="5.0.0" />
    <PackageReference Include="System.Reflection.Emit" Version="4.7.0" />
  </ItemGroup>

</Project>
